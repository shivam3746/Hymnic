<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pavlion.hymnic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WebPlayer" android:label="Web Player" />
        <activity android:name=".ArtistOptions" android:label="Artist Options" />
        <activity android:name=".songs_list" android:label="Songs List"/>
        <activity android:name=".Final_Player" android:label="Final Player" android:parentActivityName=".WebPlayer" />
        <activity android:name=".songs_detail"  android:parentActivityName=".songs_list"  android:label="Songs Detail"/>
    </application>

</manifest>